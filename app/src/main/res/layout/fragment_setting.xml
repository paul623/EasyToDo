<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar
        app:title="设置"
        android:fitsSystemWindows="true"
        app:titleTextColor="@color/white"
        android:background="@color/blue"
        android:layout_width="match_parent"
        android:layout_height="90dp"/>
    <com.allen.library.SuperTextView
        android:id="@+id/stv_1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:sLeftViewMarginLeft="15dp"
        app:sLeftTextString="背景设置"
        app:sLeftTextSize="16sp"
        android:paddingLeft="10dp"
        app:sBottomDividerLineMarginLeft="13dp"
        app:sDividerLineColor="#CFCFCF"
        app:sRightIconRes="@drawable/ic_right"
        app:sRightIconHeight="20dp"
        app:sRightIconWidth="20dp"
        android:background="@color/white" />
    <com.allen.library.SuperTextView
        android:id="@+id/stv_2"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:sLeftViewMarginLeft="15dp"
